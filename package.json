{
	"name": "temperature-website",
	"homepage": "https://paletten.oliverflecke.me",
	"version": "1.5.0",
	"private": true,
	"scripts": {
		"dev": "concurrently --kill-others --names \"JS,CSS\" -c \"bgBlue.bold,bgGreen.bold\" \"yarn start\" \"yarn css\"",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"predeploy": "yarn run build",
		"deploy": "gh-pages -d build",
		"lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
		"css": "TAILWIND_MODE=watch yarn postcss src/styles.css -o src/index.css -v --watch"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@visx/axis": "^2.4.0",
		"@visx/group": "^2.1.0",
		"@visx/responsive": "^2.4.1",
		"@visx/scale": "^2.2.2",
		"@visx/shape": "^2.4.0",
		"async-mqtt": "^2.6.1",
		"beautiful-react-hooks": "^1.0.2",
		"date-fns": "^2.27.0",
		"mqtt": "^4.3.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-icons": "^4.3.1",
		"react-scripts": "5.0.0"
	},
	"devDependencies": {
		"@fullhuman/postcss-purgecss": "^4.1.3",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.0.3",
		"@types/node": "^17.0.4",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"@types/styled-components": "^5.1.19",
		"@typescript-eslint/eslint-plugin": "^5.8.0",
		"@typescript-eslint/parser": "^5.8.0",
		"autoprefixer": "^10.4.0",
		"concurrently": "^6.5.1",
		"eslint": "^8.5.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"gh-pages": "^3.2.3",
		"postcss": "^8.4.5",
		"postcss-cli": "^9.1.0",
		"postcss-hash": "^3.0.0",
		"postcss-import": "^14.0.2",
		"postcss-nesting": "^10.1.0",
		"prettier": "^2.5.1",
		"react-snap": "^1.23.0",
		"tailwindcss": "^3.0.7",
		"typescript": "^4.5.4"
	}
}
